+  void InAppWebView::loadFile(const std::string& assetFilePath) const
+  {
+    if (!webView) {
+      return;
+    }
+
+    WCHAR* buf = new WCHAR[32768];
+    GetModuleFileName(NULL, buf, 32768);
+    std::filesystem::path exeAbsPath = std::wstring(buf);
