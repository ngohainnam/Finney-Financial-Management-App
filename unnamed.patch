+class _QuizResultPageState extends State<QuizResultPage> {
+  late double percent;
+  String feedbackMessage = '';
+  int? highestScore;
+
+  @override
+  void initState() {
+    super.initState();
+    percent = widget.score / widget.total;
+    _saveQuizResult();
+    _loadHighestScore();
+  }
+
+  @override
+  void didChangeDependencies() {
+    super.didChangeDependencies();
+    feedbackMessage = _getFeedback(percent);
