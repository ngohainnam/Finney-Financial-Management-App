+      final quizResult = QuizResult(
+        id: '',
+        score: widget.score,
+        totalQuestions: widget.total,
+        timestamp: DateTime.now(),
+      );
+      await StorageManager().learningService.saveQuizResult(quizResult);
+    } catch (e) {
+      debugPrint('Error saving quiz result: $e');
+    }
