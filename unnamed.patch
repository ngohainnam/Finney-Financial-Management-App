+    if (_selectedTransactions.isEmpty) return;
+
+    return showDialog(
+      context: context,
+      builder: (context) => AlertDialog(
+        title: Text(LocaleData.deleteTransactions.getString(context)),
+        content: Text(
+          LocaleData.confirmDeleteTransactions
